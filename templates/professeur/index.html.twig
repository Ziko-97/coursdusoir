    <!-- END: Theme JS-->

    <!-- BEGIN: Page JS-->
   <!-- <script src="{{ asset('app-assets/js/scripts/pages/dashboard-analytics.js') }}"></script>-->
    <!-- END: Page JS-->
{% endblock %}